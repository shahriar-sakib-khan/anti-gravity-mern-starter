meta {
  name: Login
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/auth/login
  body: json
  auth: none
}

body:json {
  {
    "email": "test@example.com",
    "password": "Password123!"
  }
}

script:post-response {
  if (res.status === 200 && res.body.accessToken) {
    bru.setEnvVar("accessToken", res.body.accessToken);
  }
}
